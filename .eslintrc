{
 "extends": [
   "eslint:recommended",
   "plugin:import/errors",
   "plugin:import/warnings"
 ],
 "parserOptions": {
   "ecmaVersion": 6,
   "sourceType": "module",
   "ecmaFeatures": {
     "jsx": true
   }
 },
 "env": {
   "browser": true,
   "node": true,
   "jquery": true,
   "mocha": true
 },
 "rules": {
   "quotes": 0,
   "no-console": 0,
   "no-debugger": 1,
   "indent": ["error", 2],
   "semi": [1, "always"],
   "no-trailing-spaces": 0,
   "eol-last": 0,
   "no-unused-vars": 0,
   "no-underscore-dangle": 0,
   "no-alert": 0,
   "no-lone-blocks": 0,
   "jsx-quotes": 1,
   "no-undef": 0,
 }
}
